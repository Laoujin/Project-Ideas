<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Class</Title>
			<Shortcut>class</Shortcut>
			<Description>Code snippet for creating a new class</Description>
			<Author>Van Schandevijl Wouter</Author>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>name</ID>
					<ToolTip>Class name</ToolTip>
					<Default>SomeType</Default>
				</Literal>
        <Literal>
          <ID>modifier</ID>
          <ToolTip>Access modifier of the class</ToolTip>
          <Default>Public</Default>
        </Literal>
        <Literal>
          <ID>end</ID>
        </Literal>
			</Declarations>
			<Code Language="VB"><![CDATA[''' <summary>
''' 
''' </summary>
  $modifier$ Class $name$
  
	End Class]]></Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>